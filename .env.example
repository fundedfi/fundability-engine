# Fundability Snapshot Engine - Environment Variables
# Copy this file to .env.local for local development
# For production, set these in Vercel dashboard

# ======================
# CORE CONFIGURATION
# ======================

# Node environment (development, production)
NODE_ENV=production

# API version
API_VERSION=fs_engine_v1.0

# ======================
# WEBHOOK INTEGRATIONS
# ======================

# Slack webhook URL for notifications
# Get from: https://api.slack.com/messaging/webhooks
SLACK_WEBHOOK_URL=

# Discord webhook URL for notifications  
# Get from: Server Settings > Integrations > Webhooks
DISCORD_WEBHOOK_URL=

# HubSpot webhook URL for contact updates
# Get from: Workflows > Create webhook action
HUBSPOT_WEBHOOK_URL=

# Generic webhook URL for custom integrations
CUSTOM_WEBHOOK_URL=

# Webhook secret for authentication (optional)
WEBHOOK_SECRET=

# ======================
# AUTHENTICATION (Optional)
# ======================

# API key for endpoint protection
# Generate: openssl rand -hex 32
API_KEY=

# JWT secret for token-based auth
# Generate: openssl rand -hex 64
JWT_SECRET=

# ======================
# RATE LIMITING
# ======================

# Max requests per IP per hour
RATE_LIMIT_PER_HOUR=1000

# Max concurrent batch processing
BATCH_CONCURRENCY=10

# Delay between batch chunks (ms)
BATCH_DELAY_MS=100

# ======================
# DATABASE (Future)
# ======================

# PostgreSQL connection string
# Format: postgresql://user:password@host:5432/database
DATABASE_URL=

# MongoDB connection string
# Format: mongodb+srv://user:password@cluster.mongodb.net/database
MONGODB_URI=

# Redis connection string for caching
# Format: redis://user:password@host:6379
REDIS_URL=

# ======================
# ANALYTICS & MONITORING
# ======================

# Vercel Analytics
VERCEL_ANALYTICS_ID=

# Sentry DSN for error tracking
# Get from: https://sentry.io
SENTRY_DSN=

# LogRocket app ID for session replay
# Get from: https://logrocket.com
LOGROCKET_APP_ID=

# PostHog API key for product analytics
# Get from: https://posthog.com
POSTHOG_API_KEY=

# ======================
# EMAIL NOTIFICATIONS
# ======================

# SendGrid API key
# Get from: https://sendgrid.com
SENDGRID_API_KEY=

# From email address for notifications
FROM_EMAIL=noreply@yourdomain.com

# Admin email for alerts
ADMIN_EMAIL=admin@yourdomain.com

# ======================
# STORAGE & CDN
# ======================

# AWS S3 bucket for file storage
AWS_S3_BUCKET=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

# Cloudflare R2 for file storage (S3-compatible)
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=

# ======================
# CRM INTEGRATIONS
# ======================

# HubSpot API key
# Get from: Settings > Integrations > Private Apps
HUBSPOT_API_KEY=

# Salesforce credentials
SALESFORCE_CLIENT_ID=
SALESFORCE_CLIENT_SECRET=
SALESFORCE_USERNAME=
SALESFORCE_PASSWORD=
SALESFORCE_SECURITY_TOKEN=

# Airtable credentials
# Get from: https://airtable.com/create/tokens
AIRTABLE_API_KEY=
AIRTABLE_BASE_ID=
AIRTABLE_TABLE_NAME=

# Google Sheets integration
# Create service account: https://console.cloud.google.com
GOOGLE_SERVICE_ACCOUNT_EMAIL=
GOOGLE_PRIVATE_KEY=
GOOGLE_SHEET_ID=

# ======================
# CREDIT BUREAU APIs (Future)
# ======================

# Experian API credentials
EXPERIAN_API_KEY=
EXPERIAN_USERNAME=
EXPERIAN_PASSWORD=

# Equifax API credentials
EQUIFAX_API_KEY=
EQUIFAX_CLIENT_ID=
EQUIFAX_CLIENT_SECRET=

# TransUnion API credentials
TRANSUNION_API_KEY=
TRANSUNION_SUBSCRIBER_CODE=

# ======================
# PAYMENT PROCESSING
# ======================

# Stripe for subscription management
STRIPE_SECRET_KEY=
STRIPE_PUBLISHABLE_KEY=
STRIPE_WEBHOOK_SECRET=

# ======================
# FEATURE FLAGS
# ======================

# Enable webhook notifications
ENABLE_WEBHOOKS=true

# Enable analytics tracking
ENABLE_ANALYTICS=true

# Enable email notifications
ENABLE_EMAIL=false

# Enable rate limiting
ENABLE_RATE_LIMITING=true

# Enable authentication
ENABLE_AUTH=false

# ======================
# MACHINE LEARNING (Future)
# ======================

# ML model endpoint URL
ML_MODEL_ENDPOINT=

# ML model API key
ML_MODEL_API_KEY=

# Enable ML predictions (when available)
ENABLE_ML_PREDICTIONS=false

# ======================
# CUSTOM BUSINESS LOGIC
# ======================

# Minimum fundability score for auto-approval
MIN_AUTO_APPROVAL_SCORE=70

# Maximum utilization for prime tier
MAX_PRIME_UTILIZATION=30

# Minimum credit score for business funding
MIN_BUSINESS_CREDIT_SCORE=650

# Default funding limit by tier (in dollars)
TIER_1_MAX_FUNDING=150000
TIER_2_MAX_FUNDING=75000
TIER_3_MAX_FUNDING=50000
TIER_4_MAX_FUNDING=25000

# ======================
# DEVELOPMENT ONLY
# ======================

# Enable debug logging
DEBUG=false

# Enable CORS for all origins (dev only)
ALLOW_ALL_CORS=false

# Mock webhook responses
MOCK_WEBHOOKS=false
